{
  "name": "fraudbusters-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development",
    "build": "ng build --prod",
    "test": "ng test",
    "lint-cmd": "eslint \"src/**/*.{ts,js,html}\" --max-warnings 1072",
    "lint-cache-cmd": "npm run lint-cmd -- --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier-preset": "prettier \"**/*.{html,js,ts,css,scss,md,json,prettierrc,svg,huskyrc}\"",
    "prettier": "npm run prettier-preset -- --list-different",
    "prettier:fix": "npm run prettier-preset -- --write",
    "fix": "npm run lint:fix; npm run prettier:fix",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "swagger-codegen": "ts-node --project tools/tsconfig.json tools/swagger-codegen.ts",
    "codegen": "run-p swagger-codegen",
    "parallel-cmd": "concurrently --kill-others-on-fail --prefix-colors magenta,green,red,yellow,blue",
    "ci:test": "npm run test -- --configuration=ci",
    "ci:check": "npm run parallel-cmd -- --names PRETTIER,LINT \"npm run prettier\" \"npm run lint-cmd -- --quiet\""
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-material-components/datetime-picker": "7.0.1",
    "@angular/animations": "13.3.11",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.3.11",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "angular-file": "3.6.0",
    "apexcharts": "3.35.3",
    "coerce-property": "0.3.2",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "eslint-plugin": "1.0.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0",
    "keycloak-angular": "10.0.2",
    "keycloak-js": "18.0.1",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "ng-apexcharts": "1.7.1",
    "ngx-papaparse": "4.0.4",
    "rxjs": "6.6.7",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "uuid": "3.4.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.8",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.8",
    "@angular/compiler-cli": "13.3.11",
    "@types/jasmine": "3.6.11",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "12.20.55",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "codelyzer": "6.0.2",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "4.3.8",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "protractor": "7.0.0",
    "shelljs": "0.8.5",
    "ts-node": "8.3.0",
    "tslint-config-prettier": "1.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,js,ts,css,scss,md,json,prettierrc,svg,huskyrc}": "prettier --write"
  }
}
